{
  "name": "react-imgpro",
  "version": "1.0.0",
  "description": "Image processing component for React",
  "main": "index.js",
  "flat": true,
  "files": [
    "index.js"
  ],
  "keywords": [
    "image processing",
    "image",
    "jimp",
    "sepia",
    "colors",
    "invert",
    "image component"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nitin42/react-imgpro"
  },
  "author": "Nitin Tulswani",
  "license": "MIT",
  "dependencies": {
    "@polymer/iron-image": "next",
    "@polymer/polymer": "^3.0.0-pre.1",
    "@webcomponents/webcomponentsjs": "^1.0.10",
    "rollup": "^0.49.3"
  },
  "scripts": {
    "start": "NODE_ENV=production ./node_modules/.bin/webpack-dev-server --content-base ./public --config ./webpack.config.dev.js",
    "postinstall": "yarn run rollup && cd workers && yarn",
    "prebuild": "rm -rf ./build",
    "rollup": "rollup node_modules/@polymer/iron-image/iron-image.js --output.format iife --output.file node_modules/iron-image.js",
    "build": "rollup index.js --name ProcessImage --output.format iife --output.file index.build.js"
  },
  "resolutions": {
    "@webcomponents/webcomponentsjs": "1.0.10"
  }
}
